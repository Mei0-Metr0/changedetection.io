# Endereço de rede para comunicação entre contêineres
CDIO_BASE_URL="http://changedetection:5000"

# Chave de API do changedetection.io
CDIO_API_KEY=""

# Tag padrão para agrupar as URLs monitoradas
CDIO_TAG="estudenautfpr"

# --- CONTROLE DE EXECUÇÃO ---
# Defina quais fases executar, separadas por vírgula.
# Fase 0: Limpeza de Duplicatas
# Fase 1: Coleta de URLs
# Fase 2: Importação de novas URLs para a API
# Exemplos:
# RUN_PHASES=0,1,2 (executa tudo em sequência)
# RUN_PHASES=1      (executa apenas a coleta)
# RUN_PHASES=2      (executa apenas a importação, usando o último arquivo 'urls_coletadas.txt')
# RUN_PHASES=0      (executa apenas a limpeza)
RUN_PHASES=0,1,2

# Ativa o filtro de ano na Fase 1.
# Valores válidos: True, False
FILTRAR_POR_ANO=True